<?xml version="1.0" encoding="UTF-8"?>
<ti:app xmlns:ti="http://www.webcapsule.com">
<deployment-targets>
    <target device="mobileweb">false</target>
    <target device="ipad">false</target>
    <target device="blackberry">false</target>
    <target device="android">true</target>
    <target device="iphone">false</target>
</deployment-targets>

<id>com.looprecur.webcapsulemobile</id>
<name>webcapsule-mobile</name>
<version>1.0</version>
<publisher>taylorsmith</publisher>
<url>www.looprecur.com</url>
<description>No description provided</description>
<copyright>2011 by taylorsmith</copyright>
<icon>default_app_logo.png</icon>
    <persistent-wifi>true</persistent-wifi>
    <prerendered-icon>false</prerendered-icon>
    <statusbar-style>default</statusbar-style>
    <statusbar-hidden>false</statusbar-hidden>
    <fullscreen>true</fullscreen>
    <navbar-hidden>true</navbar-hidden>
    <analytics>false</analytics>
    <guid>98c7618d-7788-443c-a9e7-73ad588a55eb</guid>
    <property name="ti.facebook.appid">147009708687795</property>
    <iphone>
        <orientations device="iphone">
            <orientation>Ti.UI.PORTRAIT</orientation>
        </orientations>
    </iphone>
    <android xmlns:android="http://schemas.android.com/apk/res/android">
        <manifest>
            <uses-permission android:name="android.permission.INTERNET"/>
            <application>
                <activity
                    android:configChanges="keyboardHidden|orientation"
                    android:label="webcapsule-mobile"
                    android:name=".WebcapsuleMobileActivity"
                    android:screenOrientation="portrait" android:theme="@style/Theme.Titanium">
                    <intent-filter>
                        <action android:name="android.intent.action.MAIN"/>
                        <category android:name="android.intent.category.LAUNCHER"/>
                    </intent-filter>
                </activity>
                <activity android:label="webcapsule-mobile"
                    android:name="OAUTHActivity" android:screenOrientation="portrait">
                    <intent-filter>
                        <action android:name="android.intent.action.VIEW"/>
                        <category android:name="android.intent.category.DEFAULT"/>
                        <category android:name="android.intent.category.BROWSABLE"/>
                        <data android:host="oauth" android:scheme="startonuri"/>
                    </intent-filter>
                </activity>
                <activity
                    android:name="ti.modules.titanium.facebook.FBActivity" android:theme="@android:style/Theme.Translucent.NoTitleBar.Fullscreen"/>
            </application>
            <supports-screens android:anyDensity="true"
                android:largeScreens="true" android:normalScreens="true"
                android:resizeable="false" android:smallScreens="false"/>
        </manifest>
    </android>
    <modules/>
</ti:app>
